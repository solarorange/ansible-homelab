# DumbAssets Homepage Service Configuration
# Generated by Ansible - Do not edit manually

- name: DumbAssets
  description: {{ dumbassets_homepage_description }}
  icon: {{ dumbassets_homepage_icon }}
  group: {{ dumbassets_homepage_group }}
  url: https://{{ dumbassets_subdomain }}.{{ domain }}
  widget:
    type: dumbassets
    url: https://{{ dumbassets_subdomain }}.{{ domain }}/api/stats
    key: dumbassets_stats
  status: true
  ping: true
  widget_data:
    - dumbassets_stats 