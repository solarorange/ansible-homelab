# DumbAssets Notification Templates Configuration
# Generated by Ansible - Do not edit manually

templates:
  - name: 'dumbassets-alert'
    template: |
      **DumbAssets Alert: {{ "{{" }} .GroupLabels.alertname {{ "}}" }}**
      
      **Service:** {{ "{{" }} .GroupLabels.service {{ "}}" }}
      **Severity:** {{ "{{" }} .CommonLabels.severity {{ "}}" }}
      
      {{ "{{" }} range .Alerts {{ "}}" }}
      **Summary:** {{ "{{" }} .Annotations.summary {{ "}}" }}
      **Description:** {{ "{{" }} .Annotations.description {{ "}}" }}
      **URL:** {{ "{{" }} .Annotations.url {{ "}}" }}
      {{ "{{" }} end {{ "}}" }}
      
      **Time:** {{ "{{" }} .StartsAt | since {{ "}}" }} 