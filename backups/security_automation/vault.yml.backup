# Service Discovery Configuration
vault_npm_discovery_services:
  - name: "authentik"
    subdomain: "auth"
    port: 9000
    ssl: true
    auth: false
    health_check_path: "/health"
  - name: "grafana"
    subdomain: "grafana"
    port: 3000
    ssl: true
    auth: true
    health_check_path: "/api/health"
  - name: "portainer"
    subdomain: "portainer"
    port: 9000
    ssl: true
    auth: true
    health_check_path: "/api/status"
  - name: "homepage"
    subdomain: "dash"
    port: 3000
    ssl: true
    auth: false
    health_check_path: "/health"
  - name: "jellyfin"
    subdomain: "jellyfin"
    port: 8096
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "sonarr"
    subdomain: "sonarr"
    port: 8989
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "radarr"
    subdomain: "radarr"
    port: 7878
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "prowlarr"
    subdomain: "prowlarr"
    port: 9696
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "bazarr"
    subdomain: "bazarr"
    port: 6767
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "overseerr"
    subdomain: "overseerr"
    port: 5055
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "tautulli"
    subdomain: "tautulli"
    port: 8181
    ssl: true
    auth: true
    health_check_path: "/status"
  - name: "nextcloud"
    subdomain: "nextcloud"
    port: 80
    ssl: true
    auth: true
    health_check_path: "/status.php"
  - name: "paperless"
    subdomain: "paperless"
    port: 8010
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "immich"
    subdomain: "photos"
    port: 3001
    ssl: true
    auth: true
    health_check_path: "/health"
  - name: "vault"
    subdomain: "vault"
    port: 8200
    ssl: true
    auth: true
    health_check_path: "/v1/sys/health"
  - name: "gitlab"
    subdomain: "git"
    port: 80
    ssl: true
    auth: true
    health_check_path: "/-/health"
  - name: "harbor"
    subdomain: "registry"
    port: 80
    ssl: true
    auth: true
    health_check_path: "/api/v2.0/health"

# Security Headers Configuration
vault_npm_security_headers:
  X-Frame-Options: "SAMEORIGIN"
  X-Content-Type-Options: "nosniff"
  X-XSS-Protection: "1; mode=block"
  Referrer-Policy: "strict-origin-when-cross-origin"
  Content-Security-Policy: "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline';"
  Strict-Transport-Security: "max-age=31536000; includeSubDomains; preload"
  Permissions-Policy: "geolocation=(), microphone=(), camera=()"

