# DumbAssets Alertmanager Configuration
# Generated by Ansible - Do not edit manually

route:
  group_by: ['alertname', 'service']
  group_wait: 30s
  group_interval: 5m
  repeat_interval: 4h
  receiver: 'dumbassets-notifications'

receivers:
  - name: 'dumbassets-notifications'
    webhook_configs:
      - url: '{{ dumbassets_apprise_url }}'
        send_resolved: true
        title: '{{ "{{" }} .GroupLabels.alertname {{ "}}" }}'
        message: '{{ "{{" }} range .Alerts {{ "}}" }}{{ "{{" }} .Annotations.summary {{ "}}" }}: {{ "{{" }} .Annotations.description {{ "}}" }}{{ "{{" }} end {{ "}}" }}' 